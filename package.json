    "shrinkwrap:nexus": "npm shrinkwrap --dev && sed -i '' 's/registry\\.npmjs\\.org/nexus\\.mgmt\\.health-dev\\.dwpcloud\\.uk\\/repository\\/npm-internal/g' npm-shrinkwrap.json"
  },
